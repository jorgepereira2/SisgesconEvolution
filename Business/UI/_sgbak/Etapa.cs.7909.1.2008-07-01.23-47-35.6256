using System;
using System.Collections.Generic;
using System.Text;

namespace Marinha.Business
{
    public class Etapa
    {
        private int _id_pedidoServico;
        private string _texto;
        private DateTime _data;
        private int _diasDemora = -1;
        private string _tipo;

        public virtual string Tipo
        {
            get { return _tipo; }
            set { _tipo = value; }
        }
        
        public virtual int DiasDemora
        {
            get { return _diasDemora; }
            set { _diasDemora = value; }
        }
        public virtual DateTime Data
        {
            get { return _data; }
            set { _data = value; }
        }
        public virtual string Texto
        {
            get { return _texto; }
            set { _texto = value; }
        }

        public virtual int ID_PedidoServico
        {
            get { return _id_pedidoServico; }
            set { _id_pedidoServico = value; }
        }
    }
}
