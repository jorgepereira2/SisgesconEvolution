<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.EntradaValores,Marinha.Business" table="TB_EntradaValores" lazy="true">

		<id name="ID" column="ID_EntradaValores" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="Projeto" column="ID_Projeto" class="Marinha.Business.Projeto,Marinha.Business" />
		<many-to-one name="NaturezaDespesa" column="ID_NaturezaDespesa" class="Marinha.Business.NaturezaDespesa,Marinha.Business" />		
		<many-to-one name="Servidor" column="ID_Servidor" class="Marinha.Business.Servidor,Marinha.Business" />
		<property column="Valor" type="Decimal" name="Valor" />
		<property column="NumeroDocumento" type="String" name="NumeroDocumento" />
		<property column="Data" type="DateTime" name="Data" />
		<many-to-one name="FonteRecurso" column="ID_FonteRecurso" class="Marinha.Business.FonteRecurso,Marinha.Business" />
		<many-to-one name="PTRES" column="ID_PTRES" class="Marinha.Business.PTRES,Marinha.Business" />
		<property column="ID_TipoMovimentoFinanceiro" type="Int32" name="TipoMovimentoFinanceiro" />
    <property column="ID_TipoOperacaoFinanceira" type="Int32" name="TipoOperacaoFinanceira" />
		<many-to-one name="AutorizacaoCompra" column="ID_AutorizacaoCompra" class="Marinha.Business.AutorizacaoCompra,Marinha.Business" />
	</class>
</hibernate-mapping>
