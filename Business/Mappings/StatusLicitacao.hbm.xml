<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.StatusLicitacao,Marinha.Business" table="TB_StatusLicitacao" lazy="true">

		<id name="ID" column="ID_StatusLicitacao" type="Int32" unsaved-value="0">
			<generator class="assigned"/>
		</id>
		<property column="Descricao" type="String" name="Descricao" not-null="true" length="100" />
    <property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true"  />

    <bag name="Responsaveis" table="TB_StatusLicitacaoServidor" lazy="true"  collection-type="Marinha.Business.CustomList`1[Marinha.Business.Servidor],Marinha.Business" >
            <key>
                <column name="ID_StatusLicitacao" not-null="true"/>
            </key>
            <many-to-many class="Marinha.Business.Servidor,Marinha.Business">
                <column name="ID_Servidor" not-null="true"/>
            </many-to-many>
        </bag>
        
    </class>
</hibernate-mapping>
