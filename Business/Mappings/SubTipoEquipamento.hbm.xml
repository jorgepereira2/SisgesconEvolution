<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.SubTipoEquipamento,Marinha.Business" table="TB_SubTipoEquipamento" lazy="true">

		<id name="ID" column="ID_SubTipoEquipamento" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="TipoEquipamento" column="ID_TipoEquipamento" class="Marinha.Business.TipoEquipamento,Marinha.Business" />
		<property column="Descricao" type="String" name="Descricao" not-null="true" length="50" />
        <property column="FlagProgem" type="Boolean" name="FlagProgem" not-null="true" />
		<property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true" />
		
	</class>
</hibernate-mapping>
