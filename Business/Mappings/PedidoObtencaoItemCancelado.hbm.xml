		<property column="Justificativa" type="String" name="Justificativa" not-null="true" length="500" />
		<property column="Quantidade" type="Decimal" name="Quantidade" not-null="true" />		
        <property column="ID_OrigemMaterial" type="Int32" name="OrigemMaterial" not-null="true" />
    <many-to-one name="AutorizacaoCompra" column="ID_AutorizacaoCompra" class="Marinha.Business.AutorizacaoCompra,Marinha.Business" />
	</class>
</hibernate-mapping>
