<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.Rotina,Marinha.Business" table="TB_Rotina" lazy="true">

		<id name="ID" column="ID_Rotina" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<property column="Descricao" type="String" name="Descricao" not-null="true" />
		<property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true" />

        <bag name="CategoriasServico" table="TB_RotinaCategoriaServico" collection-type="Marinha.Business.CustomList`1[Marinha.Business.RotinaCategoriaServico],Marinha.Business" inverse="true" lazy="true" >
            <key column="ID_Rotina" />
            <one-to-many class="Marinha.Business.RotinaCategoriaServico,Marinha.Business" />
        </bag>

	</class>
</hibernate-mapping>
