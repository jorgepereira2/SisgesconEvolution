<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.LicitacaoDocumento,Marinha.Business" table="TB_LicitacaoDocumento" lazy="true">

		<id name="ID" column="ID_LicitacaoDocumento" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="Licitacao" column="ID_Licitacao" class="Marinha.Business.Licitacao,Marinha.Business" />
        <property column="Documento" type="BinaryBlob" name="Documento" />
        <property column="Nome" type="String" name="Nome" />
		
	</class>
</hibernate-mapping>
