<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.PedidoServicoEquipamento,Marinha.Business" table="TB_PedidoServicoEquipamento" lazy="true">

		<id name="ID" column="ID_PedidoServicoEquipamento" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="PedidoServico" column="ID_PedidoServico" class="Marinha.Business.PedidoServico,Marinha.Business" />
		<many-to-one name="Equipamento" column="ID_Equipamento" class="Marinha.Business.Equipamento,Marinha.Business" />
		<property column="Quantidade" type="Int32" name="Quantidade" />
		<property column="DefeitoReclamado" type="String" name="DefeitoReclamado" />
		<property column="NumeroSerie" type="String" name="NumeroSerie" length="30" />
  
	</class>
</hibernate-mapping>