<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.PedidoObtencaoPagamento,Marinha.Business" table="TB_PedidoObtencaoPagamento" lazy="true">

		<id name="ID" column="ID_PedidoObtencaoPagamento" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="PedidoObtencao" column="ID_PedidoObtencao" class="Marinha.Business.PedidoObtencao,Marinha.Business" />
        <many-to-one name="Status" column="ID_StatusPedidoObtencao" class="Marinha.Business.StatusPedidoObtencao,Marinha.Business" />
        <property column="NumeroNF" type="String" name="NumeroNF" />    
		<property column="Valor" type="Decimal" name="Valor"  />    	
		<property column="NumeroOrdemBancaria" type="String" name="NumeroOrdemBancaria" />
        <many-to-one name="Empenho" column="ID_PedidoObtencaoEmpenho" class="Marinha.Business.PedidoObtencaoEmpenho,Marinha.Business" />
	</class>
</hibernate-mapping>
