<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.CotaMaterial,Marinha.Business" table="TB_CotaMaterial" lazy="true">

		<id name="ID" column="ID_CotaMaterial" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="Material" column="ID_Material" class="Marinha.Business.ServicoMaterial,Marinha.Business" />
		<many-to-one name="Celula" column="ID_Celula" class="Marinha.Business.Celula,Marinha.Business" />
		<property column="Ano" type="Int32" name="Ano" not-null="true" />
		<property column="Mes" type="Int32" name="Mes" not-null="true" />
		<property column="Quantidade" type="Decimal" name="Quantidade" not-null="true" />
		
	</class>
</hibernate-mapping>
