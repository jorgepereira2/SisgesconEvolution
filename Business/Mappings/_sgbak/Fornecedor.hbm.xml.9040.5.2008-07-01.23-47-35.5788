<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.Fornecedor,Marinha.Business" table="TB_Fornecedor" lazy="true">

		<id name="ID" column="ID_Fornecedor" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<many-to-one name="TipoFornecedor" column="ID_TipoFornecedor" class="Marinha.Business.TipoFornecedor,Marinha.Business" />
		<property column="CNPJ" type="String" name="CNPJ" not-null="true" length="30" />
		<property column="RazaoSocial" type="String" name="RazaoSocial" not-null="true" length="120" />
		<property column="Observacao" type="String" name="Observacao" length="500" />
		<property column="NumeroBanco" type="String" name="NumeroBanco" length="4" />
		<property column="ContaCorrente" type="String" name="ContaCorrente" length="15" />
		<property column="Agencia" type="String" name="Agencia" length="12" />
		<property column="Endereco" type="String" name="Endereco" length="120" />
		<property column="Bairro" type="String" name="Bairro" length="50" />
		<many-to-one name="Municipio" column="ID_Municipio" class="Marinha.Business.Municipio,Marinha.Business" />
		<many-to-one name="Estado" column="ID_Estado" class="Marinha.Business.Estado,Marinha.Business" />
		<property column="CEP" type="String" name="CEP" length="20" />
		<property column="Telefone" type="String" name="Telefone" length="50" />
		<property column="Fax" type="String" name="Fax" length="50" />
		<property column="Email" type="String" name="Email" length="80" />
		<property column="HomePage" type="String" name="HomePage" length="120" />
		<property column="ValidadeCertidaoReceitaFederal" type="DateTime" name="ValidadeCertidaoReceitaFederal" />
		<property column="ValidadeCertidaoFGTS" type="DateTime" name="ValidadeCertidaoFGTS" />
		<property column="ValidadeCertidaoDividaUniao" type="DateTime" name="ValidadeCertidaoDividaUniao" />
		<property column="DescricaoMaterialFornecido" type="String" name="DescricaoMaterialFornecido" length="800" />
		<property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true" />
    <property column="FlagOM" type="Boolean" name="FlagOM" not-null="true" />
    <property column="FlagOptanteSimples" type="Boolean" name="FlagOptanteSimples" not-null="true" />
    <property column="ID_TipoPessoa" type="Int32" name="TipoPessoa" not-null="true" />
    
		<property column="MotivoInativo" type="String" name="MotivoInativo" length="200" />
        <bag name="Contatos" table="TB_FornecedorContato" collection-type="Marinha.Business.CustomList`1[Marinha.Business.FornecedorContato],Marinha.Business" inverse="true" >
            <key column="ID_Fornecedor" />
            <one-to-many class="Marinha.Business.FornecedorContato,Marinha.Business" />
        </bag>
    </class>
</hibernate-mapping>
