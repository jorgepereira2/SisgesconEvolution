<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.Celula,Marinha.Business" table="TB_Celula" lazy="true">

		<id name="ID" column="ID_Celula" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<property column="Codigo" type="String" name="Codigo" length="5" />
		<property column="Descricao" type="String" name="Descricao" not-null="true" length="50" />
		<property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true" />
		<property column="FlagCentroCusto" type="Boolean" name="FlagCentroCusto" not-null="true" />
		<property column="FlagOficina" type="Boolean" name="FlagOficina" not-null="true" />
		<property column="Finalidade" type="String" name="Finalidade" length="500" />
		
	</class>
</hibernate-mapping>
