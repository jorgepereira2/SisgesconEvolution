<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Marinha.Business.TipoCompra,Marinha.Business" table="TB_TipoCompra" lazy="true">

		<id name="ID" column="ID_TipoCompra" type="Int32" unsaved-value="0">
			<generator class="assigned"/>
		</id>
		<property column="Descricao" type="String" name="Descricao" not-null="true" length="50" />
		<property column="FlagAtivo" type="Boolean" name="FlagAtivo" not-null="true" />
		<property column="LimiteAnual" type="Decimal" name="LimiteAnual" not-null="true" />
        
	</class>
</hibernate-mapping>
