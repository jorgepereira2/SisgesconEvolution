<%@ Control Language="C#" AutoEventWireup="true" CodeFile="menu.ascx.cs" Inherits="principal_menu" %>
<%@ Register TagPrefix="ComponentArt" Namespace="ComponentArt.Web.UI" Assembly="ComponentArt.Web.UI" %>
   <ComponentArt:Menu id="Menu1" 
      Orientation="Horizontal"
      CssClass="TopGroup"
      DefaultGroupCssClass="MenuGroup"      
      DefaultItemLookId="DefaultItemLook"
      DefaultDisabledItemLookId="DisabledItemLook"
      DefaultGroupItemSpacing="0"
      ExpandDelay="100"
      ShadowEnabled="true"
      ImagesBaseUrl="images"
      EnableViewState="false"
      ExpandTransition="Fade" ShadowColor="black"
      CollapseTransition="Fade"  
      runat="server">
        <ItemLooks>
          <ComponentArt:ItemLook LookID="DefaultItemLook" CssClass="MenuItem" HoverCssClass="MenuItemHover"          
            LabelPaddingLeft="15" LabelPaddingRight="10" LabelPaddingTop="3" LabelPaddingBottom="3" />
          <ComponentArt:ItemLook LookID="TopItem" CssClass="TopItem" HoverCssClass="TopItemHover"  
            LabelPaddingLeft="5" LabelPaddingRight="12" LabelPaddingTop="3" LabelPaddingBottom="3" />
        </ItemLooks>
    </ComponentArt:Menu>