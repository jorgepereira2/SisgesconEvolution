<%@ Control Language="C#" AutoEventWireup="true" CodeFile="MessageBox.ascx.cs" Inherits="UserControls_MessageBox" %>
<Anthem:Window runat="server" ID="winMessagebox" Width="400px" Height="90px" Modal="true" Caption="Atenção" ScrollBars="None"
    OnClientShow="window.parent.expandiframe()" AllowMinimize="false" AllowMaximize="false" AllowDrag="false" AllowResize="false" >
    <div style="text-align:left" >
    <br />
    <Anthem:Label runat="server" ID="lblMensagem" Font-Bold="true" />
    <br />
    </div>
    <br />
    <br />
    <div style="text-align:right">
        <Anthem:Button runat="server" ID="btnSim" Text="Sim" CssClass="Button" />&nbsp;
        <Anthem:Button runat="server" ID="btnNao" Text="Não" CssClass="Button" />&nbsp;
    </div>
</Anthem:Window>
