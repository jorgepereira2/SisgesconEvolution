.MenuGroup
{
  background-color:white;
  background-image:url(imagens/contextMenu/group_background.gif);
  border-width:1px; 
  border-color:#7E7E81; 
  border-style:solid;
  cursor:default; 
}

.MenuItem
{    
  color:black; 
  font-family:tahoma; 
  font-size:11px; 
  margin:1px; 
  cursor:default; 
}

.MenuItemHover 
{
  background-color:#FFEEC2; 
  color:black; 
  font-family:tahoma; 
  font-size:11px; 
  border-width:1px; 
  border-color:#4B4B6F; 
  border-style:solid;
  cursor:default; 
}

.MenuBreak
{
  background-image:url(imagens/break_bg.gif);
  width:100%;
  height:1px;
}